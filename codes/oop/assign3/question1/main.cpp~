#include "Block.cpp"
#include <iostream>
using namespace std;

int main()
{
	Rectangle rectangle;//object for class rectangle
	Block block;//object for class rectangle

	//instantiate values for declared objects
	rectangle.setWidth(60);
	rectangle.setLength(100);

	block.setWidth(60);
	block.setLength(100);
	block.setHeight(100);
    std::cout<<"dislplaying dimensions for rectangle"<<endl;
	std::cout<<"************************************"<<endl;
	std::cout<<"Width: "<<rectangle.getWidth()<<endl;
	std::cout<<"Length: "<<rectangle.getLength()<<endl;

	std::cout<<"dislplaying dimensions for block"<<endl;
	std::cout<<"************************************"<<endl;
	std::cout<<"Width: "<<block.getWidth()<<endl;
	std::cout<<"Length: "<<block.getLength()<<endl;
	std::cout<<"Height: "<<block.getHeight()<<endl;

	return 0;
}
